{
  "expo": {
    "name": "URSG",
    "slug": "URSG",
    "owner": "dosylia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-ursg.png",
    "scheme": "com.dosylia.URSG",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-ursg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.dosylia.URSG",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "515525000068-0ci6qe38vc834ei52mpj57ofmhrl1e35.apps.googleusercontent.com"
            ]
          }
        ],
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "UIBackgroundModes": ["remote-notification", "fetch"],
        "CFBundleAllowMixedLocalizations": true
      },
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.515525000068-0ci6qe38vc834ei52mpj57ofmhrl1e35"
        }
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "useNextNotificationsApi": true,
      "package": "com.dosylia.URSG",
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-ursg.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["RECEIVE_BOOT_COMPLETED", "VIBRATE"],
      "config": {
        "googleSignIn": {
          "apiKey": "YOUR_API_KEY",
          "certificateHash": "YOUR_CERTIFICATE_HASH"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.dosylia.URSG",
              "host" : "com.dosylia.URSG",
              "pathPrefix": "/"
            },
            {
              "scheme": "com.dosylia.URSG",
              "host" : "*",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-ursg.ico"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "94f54469-7266-46df-b5a2-c4628f8d9a8d"
      }
    },
    "assetBundlePatterns": [
      "assets/*"
    ]
  }
}
